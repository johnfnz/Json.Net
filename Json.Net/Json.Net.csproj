<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Json.Net</PackageId>
    <Authors>Onur Barlik</Authors>
    <Company>Onur Barlik</Company>
    <Description>A minimalistic JSON handler library.

Usage:
-------------------
class Pet
{
public int id;
public string name;
}

var petJson = Json.Serialize(pet);
var restoredPet = Json.Deserialize&lt;Pet&gt;(petJson);</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/obarlik/Json.Net</PackageProjectUrl>
    <PackageTags>json</PackageTags>
    <PackageReleaseNotes>Alpha release.</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/obarlik/Json.Net.git</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <Version>1.0.1</Version>
    <AssemblyVersion>1.0.0.1</AssemblyVersion>
    <FileVersion>1.0.1.1</FileVersion>
  </PropertyGroup>

</Project>
